@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage

<section class="accordion">
    <div class="accordion-item">
        <button class="accordion-header">O que é Submax?</button>
        <div class="accordion-content">
            <p>A Submax oferece impressões por sublimação de alta qualidade para bonés, camisetas e mais.</p>
        </div>
    </div>

    <div class="accordion-item">
        <button class="accordion-header">Como faço um pedido?</button>
        <div class="accordion-content">
            <p>Entre em contato via WhatsApp ou Instagram e enviamos o orçamento personalizado.</p>
        </div>
    </div>

    <div class="accordion-item">
        <button class="accordion-header">Quais produtos vocês personalizam?</button>
        <div class="accordion-content">
            <p>Trabalhamos com camisetas, bonés, ecobags, canecas e muito mais.</p>
        </div>
    </div>
    <script src="/js/faq.js" asp-append-version="true"></script>
</section>

